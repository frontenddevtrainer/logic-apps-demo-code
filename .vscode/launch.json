{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Python Azure Function",
      "type": "python",
      "request": "attach",
      "connect": {
        "host": "localhost",
        "port": 9091
      },
      "preLaunchTask": "func: host start",
      "justMyCode": true
    },
    {
      "name": "Run/Debug logic app InsertRows",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:azureLogicAppsStandard.pickProcess}"
    },
    {
      "name": "Run/Debug logic app DLQ",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:azureLogicAppsStandard.pickProcess}"
    },
    {
      "name": "Run/Debug logic app hands-on-demo",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:azureLogicAppsStandard.pickProcess}"
    }
  ]
}
